#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<" ⭐ Enter a digit ⭐"<<endl;
    cin>>n;
    int i = 0;
    while(i < n)//rows
    {
        int j = 0;
        while(j < n)//columns
        {
            cout<<" |_| ";
            j = j + 1;
        }
        cout<<endl;
        i = i + 1;
    }
    return 0;
}

OUTPUT :-

 ⭐ Enter a digit ⭐
5
 |_|  |_|  |_|  |_|  |_| 
 |_|  |_|  |_|  |_|  |_| 
 |_|  |_|  |_|  |_|  |_| 
 |_|  |_|  |_|  |_|  |_| 
 |_|  |_|  |_|  |_|  |_| 
