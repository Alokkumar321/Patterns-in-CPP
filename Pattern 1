#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i = 0;
    while(i<n)// printing columns
    {
        int j = 0;
        while(j<n)
        {
            cout<<" * "; // printing rows
            j = j + 1;
        }
        cout<<endl;
        i = i + 1;
    }
    return 0;
}

OUTPUT :-

5
 *  *  *  *  * 
 *  *  *  *  * 
 *  *  *  *  * 
 *  *  *  *  * 
 *  *  *  *  * 
