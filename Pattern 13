#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<" Enter a digit : ";
    cin>>n;
    int row = 1;
    while(row<=n)
    {
        int col = 1;
        while(col <= n)
        {
            char ch = 'A'+row+col-2;
            cout<<ch;
            col = col + 1;
        }
        cout<<endl;
        row = row + 1;
    }
}

OUTPUT :-

 Enter a digit : 5
ABCDE
BCDEF
CDEFG
DEFGH
EFGHI
